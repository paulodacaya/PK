<?xml version="1.0" encoding="UTF-8" ?>

<font-family 
    xmlns:android="http://schemas.android.com/apk/res/android" 
    xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <font 
        android:font="@font/brixsansregular"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/brixsansregular"
        app:fontStyle="normal"
        app:fontWeight="400" />
    
    <font 
        android:font="@font/brixsansregularitalic"
        android:fontStyle="italic"
        android:fontWeight="400"
        app:font="@font/brixsansregularitalic"
        app:fontStyle="italic"
        app:fontWeight="400" />
    
    <font 
        android:font="@font/brixsansmedium"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/brixsansmedium"
        app:fontStyle="normal"
        app:fontWeight="500" />
    
    <font 
        android:font="@font/brixsansbold"
        android:fontStyle="normal"
        android:fontWeight="800"
        app:font="@font/brixsansbold"
        app:fontStyle="normal"
        app:fontWeight="800" />
    
</font-family>